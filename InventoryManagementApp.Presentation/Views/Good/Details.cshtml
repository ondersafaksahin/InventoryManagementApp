@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@model GoodVM

<div class="col-sm-12 col-xl-12">
    <div class="bg-light rounded h-100 p-4">
        <h4 class="mb-4">Good Details</h4>
        <dl class="row mb-0">

            <dt class="col-sm-4 mb-2">ID</dt>
            <dd class="col-sm-8">@Model.ID</dd>

            <dt class="col-sm-4 mb-2">Good Code</dt>
            <dd class="col-sm-8">@Model.Code</dd>

            <dt class="col-sm-4 mb-2">Good Code</dt>
            <dd class="col-sm-8">@Model.ModelCode</dd>

            <dt class="col-sm-4 mb-2">Good Name</dt>
            <dd class="col-sm-8">@Model.Name</dd>

            <dt class="col-sm-4 mb-2">Status</dt>
            <dd class="col-sm-8">@Model.Status</dd>

            <dt class="col-sm-4 mb-2">Picture</dt>
            <dd class="col-sm-8">@Model.Picture</dd>

            <dt class="col-sm-4 mb-2">Stocking Unit</dt>
            <dd class="col-sm-8">@Model.StockingUnit</dd>

            <dt class="col-sm-4 mb-2">Consumption Unit</dt>
            <dd class="col-sm-8">@Model.ConsumptionUnit</dd>

            <dt class="col-sm-4 mb-2">Tax Percentage</dt>
            <dd class="col-sm-8">@Model.TaxPercentage</dd>

            <dt class="col-sm-4 mb-2">List Price</dt>
            <dd class="col-sm-8">@Model.ListPrice</dd>

            <dt class="col-sm-4 mb-2">List Currency</dt>
            <dd class="col-sm-8">@Model.ListCurrency</dd>

            <dt class="col-sm-4 mb-2">Barcode</dt>
            <dd class="col-sm-8">@Model.Barcode</dd>

            <dt class="col-sm-4 mb-2">Gross Weight</dt>
            <dd class="col-sm-8">@Model.GrossWeight</dd>

            <dt class="col-sm-4 mb-2">Net Weight</dt>
            <dd class="col-sm-8">@Model.NetWeight</dd>

            <dt class="col-sm-4 mb-2">Category Name</dt>
            <dd class="col-sm-8">@ViewBag.categoryName</dd>

            <dt class="col-sm-4 mb-2">SubCategory Name</dt>
            <dd class="col-sm-8">@ViewBag.subCategoryName</dd>

            <dt class="col-sm-4 mb-2">Model Name</dt>
            <dd class="col-sm-8">@ViewBag.modelName</dd>

            <dt class="col-sm-4 mb-2">Brand Name</dt>
            <dd class="col-sm-8">@ViewBag.brandName</dd>

            <dt class="col-sm-4 mb-2">Created By</dt>
            <dd class="col-sm-8">@Model.CreatedBy</dd>

            <dt class="col-sm-4 mb-2">Created Date</dt>
            <dd class="col-sm-8">@Model.CreatedDate</dd>

            <dt class="col-sm-4 mb-2">Modified By</dt>
            <dd class="col-sm-8">@Model.ModifiedBy</dd>

            <dt class="col-sm-4 mb-2">Modified Date</dt>
            <dd class="col-sm-8">@Model.ModifiedDate</dd>

        </dl>
    </div>
</div>

